{
  "manifest_version": 2,

  "name": "UT Syllabi Link",
  "description": "Adds a link to course instructor past syllabi and CVs in UT course schedule.",
  "author": "Ethan Perez",
  "version": "0.1",

  "icons": {
    "16":  "icons/icon16.png",
    "48":  "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["*://utdirect.utexas.edu/apps/registrar/course_schedule/*"],
      "js": ["js/jquery.js", "js/addLink.js"]
    }
  ]
}
