{
  "manifest_version": 2,

  "name": "TexReg",
  "description": "Adds research and organization features to The University of Texas Course Schedule",
  "author": "Ethan Perez",
  "version": "0.2",

  "icons": {
    "16":  "icons/icon16.png",
    "48":  "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["*://utdirect.utexas.edu/apps/registrar/course_schedule/*"],
      "js": ["js/jquery.js", "js/addLinks.js"]
    }
  ]
}
